{% extends "layout.twig" %}

{% block title %}{{ title }}{% endblock %}

{% block container %}
    <h1>Users</h1>
    <ul>
        {% for user in users %}
            <li>{{ user.firstname }}, {{ user.lastname }} ({{ user.email }})</li>
        {% endfor %}
    </ul>
{% endblock %}

{% block javascripts %}
    <script src="/users.min.js"></script>
    {{ parent() }}
{% endblock %}
